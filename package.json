  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chart.js": "^2.8.0",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.5.7",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "typed.js": "^2.0.10",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.0"
  }
}
